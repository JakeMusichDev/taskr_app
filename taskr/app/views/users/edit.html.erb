<h1>Edit!</h1>
<%=form_for(@user) do |f|%>
  <div><%=f.label :name%><%=f.text_field :name%></div>
  <div><%=f.label :email%><%=f.text_field :email%></div>
  <div><%=f.label :manager%><%=f.check_box :manager%></div>
  <div><%=f.label :phone%><%=f.telephone_field :phone%></div>
<%=f.submit "Change yo self"%>
<% end %>
